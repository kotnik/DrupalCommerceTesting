{
    "name": "drupal/commerce/testing",
    "description": "Testing framework for Drupal Commerce based on behat/mink",
    "keywords": ["web", "testing", "drupal commerce", "commerce"],
    "homepage": "http://commerceguys.com/",
    "type": "library",
    "license": "MIT",

    "require": {
        "php": ">=5.3.1",
        "behat/mink": ">=1.3.2",
        "fabpot/goutte": "*"
    },

    "autoload": {
        "psr-0": {
            "Drupal\\Commerce": "src/"
        }
    },

    "repositories": {
        "behat/mink-deps": {
            "type": "composer",
            "url": "behat.org"
        }
    }
}
